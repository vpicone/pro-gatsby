{
  "parser": "babel-eslint",
  "parserOptions": { "ecmaVersion": 2018 },
  "env": { "node": true, "browser": true, "es6": true },
  "rules": {
    "strict": 0,
    "react/prop-types": 0
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    },
    "linkComponents": [
      "Hyperlink",
      {
        "name": "Link",
        "linkAttribute": "to"
      }
    ]
  }
}
